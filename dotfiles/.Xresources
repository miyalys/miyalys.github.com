! anrxc X application resource database
! Source: http://sysphere.org/~anrxc/

!{{{ XFT
Xft.dpi:       96
Xft.antialias: 1
Xft.hinting:   1
Xft.hintstyle: hintfull
Xft.rgba:      rgb
!}}}

!{{{ URXVT
!
! General
URxvt.termName:           rxvt-unicode-256color
URxvt.loginShell:         true
URxvt.scrollBar:         false
URxvt.secondaryScroll:    true
URxvt.saveLines:         65535
URxvt.cursorBlink:       false
!urxvt.cursorUnderline:   true
URxvt.urgentOnBell:       true
!urxvt.override-redirect:false
!urxvt.borderLess:       false
!urxvt.internalBorder:       0
!urxvt.externalBorder:       0
!
! Extensions
!URxvt.perl-lib:        /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher
!urxvt.perl-ext-common: default,matcher,searchable-scrollback
URxvt.url-launcher:     /usr/bin/firefox
URxvt.matcher.button:  1
! Catch ugly URLs
URxvt.cutchars:        '"'()*<>[]{|}
!
! Appearance
!urxvt*font:         xft:Terminus:size=10
urxvt*font:         xft:Inconsolata:size=11
!urxvt*boldFont:     xft:Terminus:size=10:bold
!
! Cursor
URxvt.cursorColor: #DCDCCC
! URLs
URxvt..colorUL:    #ffffff
!
! Pseudo Transparency
!urxvt.shading:    50
!urxvt.transparent:true

! See more color schemes here:
! https://web.archive.org/web/20090912095618/http://phraktured.net/terminal-colors/terminal%20theme.tags.html

! Colors from molokai:
! pink: #F92672
! purple: #AE81FF

!Theme Thirteen
!*.background:  #3f3f3f
*.background:  #1f1f1f
*.foreground:  #dcdccc
*color0:     black
*color1:     orange1
*color2:     #A6E22E
*color3:     yellow1
*color4:     blue1
*color5:     magenta1
*color6:     rgb:cyan1
*color7:     white
*color8:     rgb:ff/d3/9b
*color9:     rgb:ff/82/47
!*color10:    rgb:ff/82/ab
!*color10:    #F92672 pink
!*color10:    #eeeeee    
*color10:     #AE81FF
*color11:    rgb:87/ce/fa
!*color12:    rgb:ee/ee/ee
*color12:    #F92672    
*color13:    grey93
*color14:    grey93
*color15:    grey93

!{{{ XMAN
Xman*Background:          #3F3F3F
Xman*Foreground:          #F0DFAF
Xman*bothShown:           true
Xman*topBox:              false
Xman*manualFontNormal:    -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-2
Xman*manualFontBold:      -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-iso8859-2
Xman*manualFontItalic:    -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-2
Xman*directoryFontNormal: -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-2
!}}}


!{{{ XPDF
xpdf*foreground:     black
xpdf*background:     white
xpdf*urlCommand:     /usr/local/bin/firefox %s
xpdf*antialias:      yes
xpdf*enableFreetype: yes
!}}}


!{{{ XCLOCK
xclock*update:     1
xclock*Mode:       analog
xclock*Background: #3F3F3F
xclock*Border:     #3F3F3F
xclock*Foreground: #F0DFAF
xclock*geometry:   120x120+1160+15
!}}}


!{{{ XDIARY
xdiary*geometry:   220x190+1034+15
xdiary*background: #3F3F3F
xdiary*foreground: #F0DFAF
!}}}


!{{{ XMESSAGE
Xmessage*font:                  -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-2
Xmessage*background:            #3F3F3F
Xmessage*foreground:            #F0DFAF
Xmessage*form.*.shapeStyle:     rectangle
Xmessage*Scrollbar.width:       1
Xmessage*Scrollbar.borderWidth: 0
!Xmessage.form.message.Scroll:  WhenNeeded
Xmessage*Buttons:               Quit
Xmessage*defaultButton:         Quit
!Xmessage*geometry:             +20+20
!}}}


!{{{ XCLIPBOARD
xclipboard.geometry:      287x195+993+15
XClipboard*international: true
XClipboard*font:          -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-2
XClipboard*borderColor:   #3F3F3F
XClipboard*cursorColor:   Gray93
XClipboard*Foreground:    #F0DFAF
XClipboard*Background:    #3F3F3F
XClipboard*form.*.shapeStyle: rectangle
!}}}


